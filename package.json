{
  "name": "pollen-bot",
  "version": "1.0.0",
  "description": "A Discord bot that provides pollen information for a given location.",
  "author": "Philipp Alber",
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "@discordjs/builders": "^1.6.4",
    "@discordjs/rest": "^2.0.0",
    "@types/node": "^20.4.8",
    "axios": "^1.4.0",
    "discord-api-types": "^0.37.51",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "tscpaths": "^0.0.9",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "start": "node -r tsconfig-paths/register build/bot.js",
    "dev": "nodemon --exec npx ts-node -r tsconfig-paths/register ./src/bot.ts",
    "lint": "eslint --max-warnings 0 --ext .js,.ts, src/** ",
    "prettier-check": "prettier --check ./src/**/*.{ts,js}",
    "build": "tsc --project ./tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build"
  }
}